{
  "rewrites": [
    { "source": "/api/payment/payment.php", "destination": "/api/payment/payment.php" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ],
  "routes": [
    {
      "src": "/public/.*",
      "dest": "/public/index.html",
      "methods": ["GET"]
    }
  ],
  "buildCommand": "npm install",
  "env": {
    "MARCHABB_PUBLIC_KEY": "@marchabb_public_key",
    "MARCHABB_SECRET_KEY": "@marchabb_secret_key"
  }
}
